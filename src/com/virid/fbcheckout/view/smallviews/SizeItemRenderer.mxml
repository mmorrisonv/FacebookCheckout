<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="false"
				width="21" height="21">
	<s:states>
		<s:State name="normal"/>
		<s:State name="selected"/>
		<s:State name="hovered"/>
	</s:states>
	<s:Rect width="100%" height="100%"><s:fill><s:SolidColor id="backColor" color="0xd5d5d5" color.selected="0xffffff" color.hovered="0xd8d8d8" /></s:fill></s:Rect>
	<s:Label text="{data.size}" verticalCenter="2" horizontalCenter="0" styleName="lubalin" fontSize="10"/>
	
</s:ItemRenderer>
